[autoload]

Secure="*res://Scripts/secure.gd"

[file_customization]

folder_colors={
"res://Scenes/": "orange",
"res://Scripts/": "yellow"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
